$ standard library functions

fun check_libgrace(): nothing
    var a: int;
    var c: char;
    var s: char[14];

    fun newline(): nothing
    {
        writeChar('\n');
    }
{
    a <- ascii('a');
    c <- chr(a);

    writeInteger(a);
    newline();

    writeChar(c);
    newline();

    strcpy(s, "Hello");
    writeString(s);
    newline();

    writeInteger(strlen(s));
    newline();

    writeInteger(strcmp(s, "Hello"));
    newline();

    writeInteger(strcmp("Hello", s));
    newline();

    $ 'o' - '\0' = 111
    writeInteger(strcmp(s, "Hell"));
    newline();

    $ '\0' - 'o' = -111
    writeInteger(strcmp("Hell", s));
    newline();

    strcat(s, " World\n");
    writeString(s);
}
