; ModuleID = 'grace'
source_filename = "grace"
target datalayout = "e-m:e-i8:8:32-i16:16:32-i64:64-i128:128-n32:64-S128"
target triple = "aarch64-unknown-linux-gnu"

%frame__main = type {}
%frame__main.doSomething = type { %frame__main*, i8, i32, [10 x i8] }

declare void @writeInteger(i32)

declare void @writeChar(i8)

declare void @writeString(i8*)

declare i32 @readInteger()

declare i8 @readChar()

declare void @readString(i32, i8*)

declare i32 @ascii(i8)

declare i8 @chr(i32)

declare i32 @strlen(i8*)

declare i32 @strcmp(i8*, i8*)

declare void @strcpy(i8*, i8*)

declare void @strcat(i8*, i8*)

define void @main.doSomething(%frame__main* %0) {
entry:
  %frame_struct = alloca %frame__main.doSomething, align 8
  %param_ptr = getelementptr inbounds %frame__main.doSomething, %frame__main.doSomething* %frame_struct, i32 0, i32 0
  store %frame__main* %0, %frame__main** %param_ptr, align 8
  %func_call_readChar = call i8 @readChar()
  %element_ptr = getelementptr inbounds %frame__main.doSomething, %frame__main.doSomething* %frame_struct, i32 0, i32 1
  store i8 %func_call_readChar, i8* %element_ptr, align 1
  %func_call_readInteger = call i32 @readInteger()
  %element_ptr1 = getelementptr inbounds %frame__main.doSomething, %frame__main.doSomething* %frame_struct, i32 0, i32 2
  store i32 %func_call_readInteger, i32* %element_ptr1, align 4
  %element_ptr2 = getelementptr inbounds %frame__main.doSomething, %frame__main.doSomething* %frame_struct, i32 0, i32 3
  %array_ptr = getelementptr [10 x i8], [10 x i8]* %element_ptr2, i32 0, i32 0
  call void @readString(i32 10, i8* %array_ptr)
  %element_ptr3 = getelementptr inbounds %frame__main.doSomething, %frame__main.doSomething* %frame_struct, i32 0, i32 1
  %l_value = load i8, i8* %element_ptr3, align 1
  call void @writeChar(i8 %l_value)
  call void @writeChar(i8 10)
  %element_ptr4 = getelementptr inbounds %frame__main.doSomething, %frame__main.doSomething* %frame_struct, i32 0, i32 2
  %l_value5 = load i32, i32* %element_ptr4, align 4
  call void @writeInteger(i32 %l_value5)
  call void @writeChar(i8 10)
  %element_ptr6 = getelementptr inbounds %frame__main.doSomething, %frame__main.doSomething* %frame_struct, i32 0, i32 3
  %array_ptr7 = getelementptr [10 x i8], [10 x i8]* %element_ptr6, i32 0, i32 0
  call void @writeString(i8* %array_ptr7)
  call void @writeChar(i8 10)
  ret void
}

define i32 @main() {
entry:
  %frame_struct = alloca %frame__main, align 8
  call void @main.doSomething(%frame__main* %frame_struct)
  ret i32 0
}
